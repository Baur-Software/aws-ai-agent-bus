# Development environment configuration
# SAFE FOR PUBLIC REPOS - Contains no secrets, only localhost URLs and feature flags

# Enable mock data for development
VITE_USE_MOCK_DATA=true

# Feature-specific mock data controls
VITE_MOCK_WORKFLOWS=true
VITE_MOCK_CONTEXTS=true
VITE_MOCK_MCP=true
VITE_MOCK_OAUTH=true
VITE_MOCK_EVENTS=true

# Dashboard server connection
VITE_DASHBOARD_SERVER_URL=http://localhost:3001

# MCP server connection (for direct MCP calls)
VITE_MCP_SERVER_URL=http://localhost:3002

# Development features
VITE_DEV_MODE=true
VITE_SHOW_DEBUG_INFO=true

# UI Development settings
VITE_APP_TITLE="AI Agent Bus (Development)"
VITE_ENABLE_DEV_TOOLS=true