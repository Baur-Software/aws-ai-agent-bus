{
  "version": "1.0",
  "created": "2025-01-15T10:30:00Z",
  "name": "Google Analytics Report Generator",
  "description": "Automated workflow that fetches GA data, processes it, and sends reports",
  "nodes": [
    {
      "id": "trigger-1642235400000",
      "type": "trigger",
      "x": 100,
      "y": 100,
      "inputs": [],
      "outputs": ["output"],
      "config": {
        "name": "Start Report Generation",
        "description": "Manual trigger to start the workflow"
      }
    },
    {
      "id": "ga-top-pages-1642235401000",
      "type": "ga-top-pages",
      "x": 350,
      "y": 100,
      "inputs": ["input"],
      "outputs": ["output"],
      "config": {
        "propertyId": "GA4-PROPERTY-ID",
        "days": 30,
        "maxResults": 10
      }
    },
    {
      "id": "ga-search-data-1642235402000",
      "type": "ga-search-data",
      "x": 350,
      "y": 250,
      "inputs": ["input"],
      "outputs": ["output"],
      "config": {
        "siteUrl": "https://example.com",
        "days": 30,
        "maxResults": 20
      }
    },
    {
      "id": "data-transform-1642235403000",
      "type": "data-transform",
      "x": 600,
      "y": 175,
      "inputs": ["input"],
      "outputs": ["output"],
      "config": {
        "transformation": "merge-analytics-data",
        "outputFormat": "json"
      }
    },
    {
      "id": "artifacts-put-1642235404000",
      "type": "artifacts-put",
      "x": 850,
      "y": 100,
      "inputs": ["input"],
      "outputs": ["output"],
      "config": {
        "key": "analytics-report-{{timestamp}}",
        "contentType": "application/json"
      }
    },
    {
      "id": "email-1642235405000",
      "type": "email",
      "x": 850,
      "y": 250,
      "inputs": ["input"],
      "outputs": [],
      "config": {
        "to": "stakeholders@company.com",
        "subject": "Weekly Analytics Report - {{date}}",
        "template": "analytics-report",
        "attachReport": true
      }
    },
    {
      "id": "events-send-1642235406000",
      "type": "events-send",
      "x": 1100,
      "y": 175,
      "inputs": ["input"],
      "outputs": ["output"],
      "config": {
        "detailType": "AnalyticsReportGenerated",
        "source": "workflow-builder",
        "detail": {
          "reportType": "weekly-analytics",
          "status": "completed"
        }
      }
    }
  ],
  "connections": [
    {
      "from": "trigger-1642235400000",
      "to": "ga-top-pages-1642235401000",
      "fromOutput": "output",
      "toInput": "input"
    },
    {
      "from": "trigger-1642235400000",
      "to": "ga-search-data-1642235402000", 
      "fromOutput": "output",
      "toInput": "input"
    },
    {
      "from": "ga-top-pages-1642235401000",
      "to": "data-transform-1642235403000",
      "fromOutput": "output",
      "toInput": "input"
    },
    {
      "from": "ga-search-data-1642235402000",
      "to": "data-transform-1642235403000",
      "fromOutput": "output", 
      "toInput": "input"
    },
    {
      "from": "data-transform-1642235403000",
      "to": "artifacts-put-1642235404000",
      "fromOutput": "output",
      "toInput": "input"
    },
    {
      "from": "data-transform-1642235403000",
      "to": "email-1642235405000",
      "fromOutput": "output",
      "toInput": "input"
    },
    {
      "from": "artifacts-put-1642235404000",
      "to": "events-send-1642235406000",
      "fromOutput": "output",
      "toInput": "input"
    }
  ],
  "metadata": {
    "author": "AWS AI Agent Bus",
    "tags": ["analytics", "google-analytics", "reporting", "automation"],
    "schedule": "weekly",
    "estimatedDuration": "5-10 minutes"
  }
}