# DEV_USER_ID=user-demo-123
# DEV_ORG_ID=acme
# DEV_USER_ROLE=admin

# Production Authentication (when ENABLE_DEV_AUTH=false)
# JWT_SECRET=your-jwt-secret-here
# COGNITO_USER_POOL_ID=us-west-2_xxxxxxxxx
# COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx