# Dashboard Server Configuration

# Authentication Settings
ENABLE_DEV_AUTH=true
NODE_ENV=development

# AWS Configuration
AWS_REGION=us-west-2
AGENT_MESH_KV_TABLE=agent-mesh-dev-kv
AGENT_MESH_ARTIFACTS_BUCKET=agent-mesh-dev-artifacts
AGENT_MESH_EVENT_BUS=agent-mesh-dev-events

# Agent Storage
AGENT_MESH_AGENTS_TABLE=agent-mesh-dev-agents
AGENT_MESH_AGENT_VERSIONS_TABLE=agent-mesh-dev-agent-versions
AGENT_MESH_AGENTS_BUCKET=agent-mesh-dev-agents

# Development User Context (when ENABLE_DEV_AUTH=true)
