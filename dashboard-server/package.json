{
  "name": "ai-agent-bus-dashboard-server",
  "version": "1.0.0",
  "description": "Dashboard backend server for AI Agent Bus with WebSocket support",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "start": "bun run src/server.ts",
    "dev": "bun --watch src/server.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.911.0",
    "@aws-sdk/client-dynamodb": "^3.896.0",
    "@aws-sdk/client-eventbridge": "^3.896.0",
    "@aws-sdk/client-s3": "^3.896.0",
    "@aws-sdk/client-sfn": "^3.896.0",
    "@aws-sdk/client-sns": "^3.896.0",
    "@aws-sdk/credential-providers": "^3.896.0",
    "@aws-sdk/lib-dynamodb": "^3.896.0",
    "@aws-sdk/s3-request-presigner": "^3.896.0",
    "@aws-sdk/util-dynamodb": "^3.896.0",
    "@modelcontextprotocol/sdk": "^1.18.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "ts-node": "^10.9.2",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "y-websocket": "^3.0.0",
    "yjs": "^13.6.27"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.17",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^11.0.0",
    "@types/ws": "^8.18.1",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "dashboard",
    "websocket",
    "aws",
    "serverless",
    "ai-agents"
  ],
  "author": "AI Agent Bus Team",
