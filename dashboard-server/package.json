{
  "name": "ai-agent-bus-dashboard-server",
  "version": "1.0.0",
  "description": "Dashboard backend server for AI Agent Bus with WebSocket support",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "start": "bun run src/server.ts",
    "dev": "bun --watch src/server.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.450.0",
    "@aws-sdk/client-eventbridge": "^3.450.0",
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/client-sfn": "^3.888.0",
    "@aws-sdk/client-sns": "^3.896.0",
    "@aws-sdk/credential-providers": "^3.888.0",
    "@aws-sdk/lib-dynamodb": "^3.888.0",
    "@aws-sdk/s3-request-presigner": "^3.888.0",
    "@aws-sdk/util-dynamodb": "^3.450.0",
    "@modelcontextprotocol/sdk": "^1.18.1",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "ts-node": "^10.9.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.8.0",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.5.10",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "tsx": "^4.20.5",
    "typescript": "^5.2.2",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "dashboard",
    "websocket",
    "aws",
    "serverless",
    "ai-agents"
  ],
  "author": "AI Agent Bus Team",
  "license": "MIT"
}
