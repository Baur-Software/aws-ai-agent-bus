{
  "name": "ai-agent-bus-dashboard-server",
  "version": "1.0.0",
  "description": "Dashboard backend server for AI Agent Bus with WebSocket support",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.450.0",
    "@aws-sdk/client-eventbridge": "^3.450.0",
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/client-sfn": "^3.888.0",
    "@aws-sdk/credential-providers": "^3.888.0",
    "@aws-sdk/lib-dynamodb": "^3.888.0",
    "@aws-sdk/s3-request-presigner": "^3.888.0",
    "@aws-sdk/util-dynamodb": "^3.450.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@types/ws": "^8.5.10",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "tsx": "^4.20.5",
    "typescript": "^5.2.2"
  },
  "keywords": [
