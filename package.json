{
  "name": "agent-mesh-mcp",
  "version": "1.0.0",
  "description": "MCP server for AWS Agent Mesh",
  "main": "mcp-server/src/server.js",
  "type": "module",
  "scripts": {
    "start": "node mcp-server/src/server.js",
    "dev": "node --watch mcp-server/src/server.js",
    "start:http": "node mcp-server/src/http-server.js",
    "dev:http": "node --watch mcp-server/src/http-server.js",
    "ui:demo": "cross-env MCP_UI_ENABLED=true node mcp-server/src/http-server.js",
    "setup:ga-google-cloud": "node mcp-server/scripts/setup-ga-google-cloud.js",
    "report:users-by-country": "node mcp-server/src/reports/users-by-country.js",
    "report:users-by-country-sample": "node mcp-server/src/reports/users-by-country-sample.js",
    "tf:fmt": "terraform fmt -recursive",
    "tf:init": "cd infra/workspaces/${WS:-small/kv_store} && terraform init",
    "tf:plan": "cd infra/workspaces/${WS:-small/kv_store} && terraform plan -var env=${ENV:-dev}",
    "tf:apply": "cd infra/workspaces/${WS:-small/kv_store} && terraform apply -auto-approve -var env=${ENV:-dev}",
    "tf:destroy": "cd infra/workspaces/${WS:-small/kv_store} && terraform destroy -auto-approve -var env=${ENV:-dev}"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/client-ecs": "^3.0.0",
    "@aws-sdk/client-eventbridge": "^3.0.0",
    "@aws-sdk/client-rds-data": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/client-secrets-manager": "^3.0.0",
    "@aws-sdk/client-sfn": "^3.0.0",
    "@mcp-ui/client": "^1.0.0",
    "@mcp-ui/server": "^1.0.0",
    "@modelcontextprotocol/sdk": "^1.17.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "google-auth-library": "^9.0.0",
    "googleapis": "^140.0.0",
    "zod": "^3.22.4"
  },
  "keywords": [
    "mcp",
    "aws",
    "agent-mesh",
    "model-context-protocol",
    "dynamodb",
    "s3",
    "eventbridge",
    "ai-tools"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Baur-Software/aws-ai-agent-bus.git",
    "directory": "mcp-server"
  },
  "bugs": {
    "url": "https://github.com/Baur-Software/aws-ai-agent-bus/issues"
  },
  "homepage": "https://github.com/Baur-Software/aws-ai-agent-bus/tree/main/mcp-server#readme"
}
