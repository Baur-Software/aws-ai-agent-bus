# AWS Configuration
# Uncomment and set these if using a specific AWS profile/region
# AWS_PROFILE=your-aws-profile
AWS_REGION=us-west-2

# Agent Mesh Configuration
AGENT_MESH_ENV=prod
AGENT_MESH_KV_TABLE=agent-mesh-kv
AGENT_MESH_ARTIFACTS_BUCKET=agent-mesh-artifacts
AGENT_MESH_TIMELINE_BUCKET=agent-mesh-timeline
AGENT_MESH_EVENT_BUS=agent-mesh-events

# HTTP Server Configuration (for http-server.js)
PORT=3000

# Base Agent Model Configuration
# These environment variables define the default models for all agents
# Individual agents can override these by setting specific values in their YAML

# Primary model for complex reasoning and main tasks
AGENT_MODEL_PRIMARY=claude:sonnet-3.5

# Secondary model for simpler tasks or fallback
AGENT_MODEL_SECONDARY=claude:haiku

# Specialized model for critical or high-quality tasks
AGENT_MODEL_SPECIALIZED=claude:opus

# Local/offline fallback model
AGENT_MODEL_FALLBACK=ollama:llama3.1

# Workflow orchestration engine
AGENT_MODEL_WORKFLOW=dify:flows

# Model selection strategy
AGENT_AUTO_MODEL_SELECT=true
AGENT_COST_OPTIMIZATION=true
AGENT_QUALITY_THRESHOLD=0.85

# Environment-specific overrides
# Development: Use faster/cheaper models
AGENT_MODEL_PRIMARY_DEV=claude:haiku
AGENT_MODEL_SECONDARY_DEV=claude:haiku
AGENT_MODEL_SPECIALIZED_DEV=claude:sonnet-3.5

# Production: Use high-quality models
AGENT_MODEL_PRIMARY_PROD=claude:sonnet-3.5
AGENT_MODEL_SECONDARY_PROD=claude:haiku
AGENT_MODEL_SPECIALIZED_PROD=claude:opus

# Agent-specific overrides (uncomment to use)
# CONDUCTOR_MODEL_PRIMARY=claude:opus
# CRITIC_MODEL_PRIMARY=claude:opus
# SWEEPER_MODEL_PRIMARY=claude:haiku